<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <DataCollectionRunSettings>
    <DataCollectors>
      <DataCollector friendlyName="XPlat Code Coverage">
        <Configuration>
          <Format>cobertura,html</Format>          
          <Exclude>[*]Program,[**]*/Program.cs,[*]*.SwaggerConfiguration,[*]*.MediatRExtensions,[**]*/MediatRExtensions.cs,[MSProfessionals.Infrastructure]*</Exclude>
          <Include>[MSProfessionals.*]*</Include>
          <SingleHit>false</SingleHit>
          <UseSourceLink>false</UseSourceLink>
          <IncludeTestAssembly>false</IncludeTestAssembly>
        </Configuration>
      </DataCollector>
    </DataCollectors>
  </DataCollectionRunSettings>
</RunSettings> 
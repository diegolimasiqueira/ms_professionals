{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DatabaseSettings": {
    "Host": "${DB_HOST}",
    "Port": "${DB_PORT}",
    "Database": "${DB_NAME}",
    "Username": "${DB_USER}",
    "Password": "${DB_PASSWORD}",
    "UseSsl": false,
    "MaxRetryCount": 3,
    "CommandTimeout": 30
  }
}
